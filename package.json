    "John Resig <jeresig@gmail.com> (http://ejohn.org/) (From Kahn/live-editor)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/therebelrobot/HelpfulHint/issues"
  },
  "homepage": "https://github.com/therebelrobot/HelpfulHint",
  "devDependencies": {
    "async": "^0.9.0",
    "chai": "^2.2.0",
    "colors": "^1.0.3",
    "istanbul": "^0.3.13",
    "mocha": "^2.2.3",
    "stack-trace": "0.0.9",
    "standard": "^3.5.0"
  },
  "dependencies": {
    "commander": "^2.8.0",
    "debug-log2": "^1.0.0",
    "gulp-jshint": "^1.10.0",
    "interpolate": "^0.1.0",
    "jshint": "^2.7.0",
    "lodash": "^3.6.0",
    "vinyl-fs": "^1.0.0"
  }
}
