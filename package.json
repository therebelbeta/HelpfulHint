{
  "name": "helpfulhint",
  "version": "0.0.2",
  "description": "HelpfulHint does a line-by-line check for common beginner programming mistakes, such as misspelling, missing spaces, missing commas, etc. It is used in conjunction with JSHINT to report errors to the user.",
  "main": "dist/helpfulhint.js",
  "bin": "dist/cli.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/istanbul/lib/cli.js cover _mocha --"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therebelrobot/HelpfulHint.git"
  },
  "keywords": [
    "hint",
    "lint",
    "jslint",
    "jshint",
    "baby",
    "helpfulhint",
    "beginner",
    "learn",
    "check",
    "khan",
    "academy",
    "school",
    "cs"
  ],
  "author": "Trent Oswald <trentoswald@therebelrobot.com> (http://therebelrobot.com)",
